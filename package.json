{
  "name": "dailymood-ai",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3009",
    "dev-clean": "npm run cleanup && next dev -p 3009",
    "dev-port": "next dev -p 3000",
    "cleanup": "taskkill /f /im node.exe || true",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit --skipLibCheck",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "check-env": "node scripts/check-environment.js",
    "test-payment-flow": "node scripts/test-payment-flow.js",
    "bugbot-review": "node scripts/bugbot-review-tracker.js",
    "bugbot-detailed": "node scripts/bugbot-review-tracker.js --detailed",
    "analyze": "ANALYZE=true npm run build",
    "analyze:server": "BUNDLE_ANALYZE=server npm run build",
    "analyze:browser": "BUNDLE_ANALYZE=browser npm run build",
    "build:analyze": "ANALYZE=true npm run build",
    "build:production": "NODE_ENV=production npm run build",
    "build:app": "next build && next export && npx cap sync",
    "deploy:android": "npx cap open android",
    "deploy:ios": "npx cap open ios",
    "setup:capacitor": "npx cap init && npx cap add android && npx cap add ios"
  },
  "dependencies": {
    "@stripe/stripe-js": "^7.8.0",
    "@supabase/auth-helpers-nextjs": "^0.8.7",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.55.0",
    "@tanstack/react-query": "^5.85.5",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.294.0",
    "next": "^14.2.31",
    "openai": "^4.20.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.6.0",
    "stripe": "^14.25.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@capacitor/android": "^6.0.0",
    "@capacitor/cli": "^6.0.0",
    "@capacitor/core": "^6.0.0",
    "@capacitor/ios": "^6.0.0",
    "@next/bundle-analyzer": "^15.5.2",
    "@playwright/test": "^1.55.0",
    "@tanstack/react-query-devtools": "^5.85.5",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20.8.10",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^5.0.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.53.0",
    "eslint-config-next": "14.0.4",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "vitest": "^3.2.4"
  }
}
